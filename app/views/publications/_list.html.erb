<table>
  <tr>
    <th>Title</th>
    <th>Publisher</th>
    <th>Points</th>
  </tr>
  <% publications.each do |publication| %>
  <tr>
    <td><%= link_to publication.branch.title, publication_path(publication.id) %></td>
    <td><%= link_to publication.user.login, user_path(publication.user_id) %></td>
    <td><%= publication.points %></td>
  </tr>
  <% end %>
</table>
