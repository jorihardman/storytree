<h1>Plant Seed</h1>

<%= form_for @seed do |f| %>
  <%= f.error_messages %>
  <%= f.fields_for :seed_detail, @seed_detail do |d| %>
    <%= d.label :title %><br/>
    <%= d.text_field :title %><br/>
    <br/>
    <%= d.label :branch_length %><br/>
    <%= d.text_field :branch_length %><br/>
    <br/>
    <%= d.label :double_posting %><br/>
    <%= d.check_box :double_posting %><br/>
    <br/>
  <% end %>
  <%= f.label :story %><br/>
  <%= f.text_area :story %><br/>
  <br/>
  <%= f.submit 'Plant' %>
<% end %>
